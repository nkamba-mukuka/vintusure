{
    "hosting": {
        "target": "vintusure-cb8f0",
        "public": ".next",
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "rewrites": [
            {
                "source": "**",
                "destination": "/index.html"
            }
        ],
        "predeploy": [
            "npm run build"
        ]
    },
    "firestore": {
        "rules": "firestore.rules",
        "indexes": "firestore.indexes.json"
    },
    "storage": {
        "rules": "storage.rules"
    },
    "emulators": {
        "auth": {
            "port": 9099
        },
        "firestore": {
            "port": 8080
        },
        "hosting": {
            "port": 5050
        },
        "storage": {
            "port": 9199
        },
        "ui": {
            "enabled": true,
            "port": 4040
        }
    }
}